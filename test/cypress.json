{

  "ignoreTestFiles": "*.js",
  "testFiles": "**/*.{feature,features}",
  "viewportWidth": 1520,
  "viewportHeight": 720,
  "reporter": "cypress-multi-reporters",
  "reporterOptions": {
    "reporterEnabled": "mochawesome",
    "mochawesomeReporterOptions": {
      "reportDir": "cypress/reports/mocha",
      "quite": true,
      "overwrite": false,
      "html": false,
      "json": true
    }
  },
    "pageLoadTimeout": 100000,
    "video": false,
    "chromeWebSecurity": false
    
}
